                <artifactId>poi-ooxml</artifactId>
                <version>${poi.version}</version>
            </dependency>
            <!-- 阿里excel工具类 -->
            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>easyexcel</artifactId>
                <version>${easyexcel.version}</version>
            </dependency>
            <!-- collections工具类 -->
            <dependency>
                <groupId>commons-collections</groupId>
                <artifactId>commons-collections</artifactId>
                <version>${commons.collections.version}</version>
            </dependency>
            <!-- 解析客户端操作系统、浏览器等 -->
            <dependency>
                <groupId>eu.bitwalker</groupId>
                <artifactId>UserAgentUtils</artifactId>
                <version>${bitwalker.version}</version>
            </dependency>
            <!-- 阿里JSON解析器 -->
            <dependency>
                <groupId>com.alibaba.fastjson2</groupId>
                <artifactId>fastjson2</artifactId>
                <version>${fastjson.version}</version>
            </dependency>
            
            <!-- wls-fast -->
            <!-- Token生成与解析-->
            <dependency>
                <groupId>io.jsonwebtoken</groupId>
                <artifactId>jjwt</artifactId>
                <version>${jwt.version}</version>
            </dependency>
            <!-- io常用工具类 -->
            <dependency>
                <groupId>commons-io</groupId>
                <artifactId>commons-io</artifactId>
                <version>${commons.io.version}</version>
            </dependency>
            <!-- Swagger3依赖 -->
            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-boot-starter</artifactId>
                <version>${swagger.version}</version>
            </dependency>
            <!-- 验证码 -->
            <dependency>
                <groupId>pro.fessional</groupId>
                <artifactId>kaptcha</artifactId>
                <version>${kaptcha.version}</version>
            </dependency>
            
            <!--服务内模块模块-->
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-gencode</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-mail</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-log</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-redis</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-web</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-swagger</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-common-pulsar</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--API模块-->
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-dict-api</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--API模块-->
            <dependency>
                <groupId>plus.wls</groupId>
                <artifactId>wls-gencode</artifactId>
                <version>${project.version}</version>
            </dependency>
        
        </dependencies>
